      break;
    case 5:
      display.drawBitmap(0, 0, player_5_bitmap, 128, 64, SSD1306_WHITE);
      break;
    case 6:
      display.drawBitmap(0, 0, player_6_bitmap, 128, 64, SSD1306_WHITE);
      break;
    case 7:
      display.drawBitmap(0, 0, player_7_bitmap, 128, 64, SSD1306_WHITE);
      break;
    case 8:
      display.drawBitmap(0, 0, player_8_bitmap, 128, 64, SSD1306_WHITE);
      break;
    default:
      break;
  }

  display.display();
}
