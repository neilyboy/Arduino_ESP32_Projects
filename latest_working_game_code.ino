  display.clearDisplay();

  // Select and display the appropriate bitmap based on the number of players
  switch (numberOfPlayers) {
    case 1:
      display.drawBitmap(0, 0, player_1_bitmap, 128, 64, SSD1306_WHITE);
      break;
    case 2:
